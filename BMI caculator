height = float(input("Enter your height in cm: "))
weight = float(input("Enter your weight in kg: "))
BMI = weight / (height/100)**2
print(f"You BMI is {BMI}")
if x < 18.5:
  print('Underweight')
if x>=18.5 and x<25:
  print("Normal")
if x >= 25 and x < 30:
  print('Overweight')
if x >= 30:
  print('Obesity')
